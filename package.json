{
  "name": "tattoo-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "validate": "npm run lint && npx tsc --noEmit",
    "test": "node scripts/testing/run-tests.cjs --all",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:debug": "npx playwright test --debug",
    "test:e2e:headed": "npx playwright test --headed",
    "test:e2e:chrome": "npx playwright test --project=chromium",
    "test:e2e:firefox": "npx playwright test --project=firefox",
    "test:e2e:safari": "npx playwright test --project=webkit",
    "test:e2e:mobile": "npx playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "test:e2e:report": "npx playwright show-report",
    "test:e2e:supabase": "scripts/run-e2e-tests.sh",
    "test:e2e:ci": "npx playwright test --reporter=dot,html",
    "test:e2e:smoke": "npx playwright test --grep @smoke",
    "prepare": "npx husky",
    "postinstall": "prisma generate",
    "apply-view-fix": "node scripts/apply-view-fix.js",
    "fix-fonts": "node scripts/fix-font-loading.js",
    "dev:clean": "rm -rf .next && npm run dev",
    "type-check": "tsc --noEmit",
    "verify-mcp": "node scripts/verify-mcp-servers.js",
    "test:unit": "node scripts/testing/run-tests.cjs --unit",
    "test:single": "npx playwright test",
    "test:e2e:enhanced": "npx playwright test --config=tests/e2e/enhanced-config.ts",
    "test:e2e:enhanced:ui": "npx playwright test --config=tests/e2e/enhanced-config.ts --ui",
    "test:e2e:enhanced:debug": "npx playwright test --config=tests/e2e/enhanced-config.ts --debug",
    "test:e2e:enhanced:headed": "npx playwright test --config=tests/e2e/enhanced-config.ts --headed",
    "test:e2e:enhanced:chrome": "npx playwright test --config=tests/e2e/enhanced-config.ts --project=chromium",
    "test:e2e:enhanced:visual": "VISUAL_TESTING=true npx playwright test --config=tests/e2e/enhanced-config.ts",
    "test:e2e:enhanced:visual:update": "VISUAL_TESTING=true CREATE_MISSING_BASELINE=true npx playwright test --config=tests/e2e/enhanced-config.ts",
    "test:e2e:enhanced:perf": "npx playwright test --config=tests/e2e/enhanced-config.ts --project=performance",
    "test:e2e:enhanced:report": "npx playwright show-report",
    "test:e2e:quick": "npx playwright test --config=playwright.config.quick.ts"
  },
