# Database URLs - Using SQLite for local development
DATABASE_URL="file:./dev.db"

# Cal.com Configuration - Update with your Cal.com details
NEXT_PUBLIC_CAL_USERNAME="your-cal-username"
CAL_WEBHOOK_SECRET="your-cal-webhook-secret"

# Supabase Configuration - Required
NEXT_PUBLIC_SUPABASE_URL="https://your-project.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-anon-key"
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key"

# Other required environment variables
NEXTAUTH_SECRET="your-nextauth-secret"
NEXTAUTH_URL="http://localhost:3000"

# Stripe (if using for payments)
STRIPE_PUBLISHABLE_KEY="pk_test_..."
STRIPE_SECRET_KEY="sk_test_..."

# Email provider (Resend)
RESEND_API_KEY="re_..."

# Other services
NEXT_PUBLIC_VERCEL_URL="http://localhost:3000"
NEXT_PUBLIC_SITE_URL="http://localhost:3000"